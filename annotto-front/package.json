	"scripts": {
		"build": "react-app-rewired build",
		"lint": "yarn eslint --ext .js,.jsx src",
		"lint:fix": "yarn lint --fix",
		"mock": "node mocks/index.js",
		"mock:ci": "cross-env CI=true yarn mock || echo -e '\\e[1m\\e[33mNo mock server defined, running tests without mocks.\\e[0m'",
		"prettier": "scripts/precommitHookPrettier.sh",
		"prettier:check": "npx prettier \"*/**/*.{js,jsx,ts,tsx,json,rc}\" --check",
		"prettier:fix": "npx prettier \"*/**/*.{js,jsx,ts,tsx,json,rc}\" --write",
		"serve": "serve -s build",
		"start": "SKIP_PREFLIGHT_CHECK=true react-app-rewired start",
		"test": "react-app-rewired test --env=jest-environment-jsdom-sixteen",
		"test:ci": "cross-env CI=true yarn test --ci --reporters=default --reporters=jest-junit -b --watchAll=false --verbose --bail",
		"test:ci:coverage": "yarn test:ci --coverage",
		"test:coverage": "yarn test --coverage --watchAll=false"
	}
